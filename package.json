  "repository": {
    "type": "git",
    "url": "https://github.com/zgulsby/cursor-runpod-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/zgulsby/cursor-runpod-deploy/issues"
  },
  "homepage": "https://github.com/zgulsby/cursor-runpod-deploy#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {},
  "devDependencies": {}
}