{
  "name": "cursor-runpod-deploy",
  "version": "1.0.0",
  "private": true,
  "description": "Deploy code to Runpod serverless endpoints directly from Cursor IDE or command line",
  "type": "module",
  "main": "deploy.js",
  "workspaces": ["tools/runpod-mcp"],
  "bin": {
    "runpod-deploy": "./deploy.js"
  },
  "scripts": {
    "deploy": "node deploy.js",
    "mcp:build": "cd tools/runpod-mcp && npm run build",
    "mcp:dev": "cd tools/runpod-mcp && npm run dev"
  },
  "keywords": [
    "runpod",
    "serverless",
    "deployment",
    "cursor",
    "mcp",
    "ai",
    "gpu",
    "cloud-compute"
  ],
